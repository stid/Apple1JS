{
  "name": "apple1js",
  "version": "1.1.1",
  "description": "NodeJS Apple 1 Emulator, include basic 6820 abstraction and other core components.",
  "main": "dist-web/bundle.js",
  "module": "src/index-web.js",
  "scripts": {
    "test": "run-s eslint type-check jest",
    "test-ci": "run-s eslint type-check jest-ci",
    "build": "webpack --config webpack.config.prod.js --mode production",
    "build-dev": "webpack --config webpack.config.dev.js  --mode development",
    "build-node": "webpack --config webpack-node.config.js --mode production",
    "start-dev": "webpack serve --config webpack.config.dev.js --mode development",
    "start": "node dist/bundle.js",
    "raw-start": "node index.js",
    "server-start": "http-server ./public",
    "eslint": "eslint src --ext .ts --ext .tsx --fix",
    "profile": "node --prof dist/bundle.js",
    "jest": "jest",
    "jest-ci": "jest --ci --runInBand --reporters=default --reporters=jest-junit",
    "type-check": "tsc --build ./src/tsconfig.json"
  },
  "repository": "git+https://github.com/stid/Apple1JS.git",
  "author": "Stefano Furiosi",
  "license": "GPL-3.0",
  "devDependencies": {
    "@babel/cli": "7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/plugin-transform-async-to-generator": "7.18.6",
    "@babel/plugin-transform-runtime": "7.18.6",
    "@babel/preset-env": "7.18.6",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@babel/register": "7.18.6",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "28.1.4",
    "@types/node": "18.0.1",
    "@types/react": "^18.0.14",
    "@types/react-dom": "18.0.5",
    "@types/webpack-env": "1.17.0",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "babel-jest": "^28.1.2",
    "babel-loader": "8.2.5",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-module-resolver": "4.1.0",
    "clean-webpack-plugin": "4.0.0",
    "dependency-cruiser": "^11.11.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.2.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "gh-pages": "4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "http-server": "^14.1.1",
    "jest": "28.1.2",
    "jest-environment-jsdom": "^28.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "2.7.1",
    "terser-webpack-plugin": "^5.3.3",
    "typescript": "4.7.4",
    "webpack": "5.73.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "^4.9.3"
  },
  "dependencies": {
    "@babel/runtime": "7.18.6",
    "@babel/runtime-corejs3": "7.18.6",
    "@stitches/react": "^1.2.8",
    "core-js": "3.23.3",
    "eslint-plugin-import": "^2.26.0",
    "immer": "9.0.15",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "waait": "1.0.5"
  },
  "bugs": {
    "url": "https://github.com/stid/Apple1JS/issues"
