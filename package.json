{
  "name": "apple1js",
  "version": "1.1.1",
  "description": "NodeJS Apple 1 Emulator, include basic 6820 abstraction and other core components.",
  "main": "dist-web/bundle.js",
  "module": "src/index-web.js",
  "scripts": {
    "test": "run-s eslint type-check jest",
    "test-ci": "run-s eslint type-check jest-ci",
    "build": "webpack --config webpack.config.prod.js --mode production",
    "build-dev": "webpack --config webpack.config.dev.js  --mode development",
    "build-node": "webpack --config webpack-node.config.js --mode production",
    "start-dev": "webpack serve --config webpack.config.dev.js --mode development",
    "start": "node dist/bundle.js",
    "raw-start": "node index.js",
    "server-start": "http-server ./public",
    "eslint": "eslint src --ext .ts --ext .tsx --fix",
    "profile": "node --prof dist/bundle.js",
    "jest": "jest",
    "jest-ci": "jest --ci --runInBand --reporters=default --reporters=jest-junit",
    "type-check": "tsc --build ./src/tsconfig.json",
    "deploy": "gh-pages -d public"
  },
  "repository": "git+https://github.com/stid/Apple1JS.git",
  "author": "Stefano Furiosi",
  "license": "GPL-3.0",
  "devDependencies": {
    "@babel/cli": "7.13.16",
    "@babel/core": "7.14.0",
    "@babel/plugin-transform-async-to-generator": "7.13.0",
    "@babel/plugin-transform-runtime": "7.13.15",
    "@babel/preset-env": "7.14.1",
    "@babel/preset-react": "7.13.13",
    "@babel/preset-typescript": "7.13.0",
    "@babel/register": "7.13.16",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@types/jest": "26.0.23",
    "@types/mocha": "8.2.2",
    "@types/node": "15.0.2",
    "@types/react": "17.0.5",
    "@types/react-dom": "17.0.3",
    "@types/webpack-env": "1.16.0",
    "@typescript-eslint/eslint-plugin": "4.22.1",
    "@typescript-eslint/parser": "4.22.1",
    "babel-eslint": "10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "8.2.2",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-module-resolver": "4.1.0",
    "clean-webpack-plugin": "3.0.0",
    "eslint": "7.26.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-mocha": "8.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "gh-pages": "3.1.0",
    "html-webpack-plugin": "^5.3.1",
    "http-server": "^0.12.3",
    "jest": "26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "2.3.0",
    "terser-webpack-plugin": "^5.1.1",
    "typescript": "4.2.4",
    "webpack": "5.36.2",
    "webpack-cli": "4.7.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@babel/runtime": "7.14.0",
    "@babel/runtime-corejs3": "7.14.0",
    "@stitches/react": "^0.1.9",
    "core-js": "3.12.1",
    "immer": "9.0.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "waait": "1.0.5"
  },
  "bugs": {
